<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="better_zip_form_extend">
            <field name="name">res.better.zip.form_extend</field>
            <field name="model">res.better.zip</field>
            <field name="inherit_id" ref="base_location.better_zip_form"/>
            <field name="arch" type="xml">
                <field name="state_id" position="after">
                    <field name="county_province"/>
                    <field name="county_province_code"/>
                    <field name="community"/>
                    <field name="community_code"/>
                    <field name="latitude"/>
                    <field name="longitude"/>
                    <field name="accuracy"/>
                </field>
            </field>
        </record>

        <record model="ir.ui.view" id="better_zip_tree_extend">
            <field name="name">res.better.zip.tree_extend</field>
            <field name="model">res.better.zip</field>
            <field name="inherit_id" ref="base_location.better_zip_tree"/>
            <field name="arch" type="xml">
                <field name="state_id" position="after">
                    <field name="county_province"/>
                    <field name="community"/>
                </field>
            </field>
        </record>

    </data>
</openerp>

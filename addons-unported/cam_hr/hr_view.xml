        <record id="edit_my_holiday_form" model="ir.ui.view">
            <field name="name">Leave Request</field>
            <field name="model">hr.holidays</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <form string="Leave Request" version="7.0">
                <header>
                    <button string="Confirm" name="confirm" states="draft" type="workflow" class="oe_highlight"/>
                    <button string="Approve" name="validate" states="confirm" type="workflow" groups="base.group_hr_manager" class="oe_highlight"/>
                    <button string="Validate" name="second_validate" states="validate1" type="workflow" groups="base.group_hr_manager" class="oe_highlight"/>
                    <button string="Refuse" name="refuse" states="confirm,validate1,validate" type="workflow" groups="base.group_hr_manager"/>
                    <button string="Reset to New" name="reset" states="refuse"/>
                    <field name="state" widget="statusbar" statusbar_visible="draft,confirm,validate" statusbar_colors='{"confirm":"blue","validate1":"blue","refuse":"red"}'/>
                </header>
                <sheet string="Leave Request">
                    <group style="width:30%%">
                        <field name="employee_id" required="1" groups="base.group_hr_manager" on_change="onchange_employee(employee_id)"/>
                        <field name="date_from" string="Start Datum" widget="date" required="1" />
                        <field name="date_to" string="End Datum" widget="date" required="1"/>
                        <field name="half_day"/>                        
                        <field name="holiday_status_id" invisible="1" context="{'employee_id':employee_id}"/>
                        <field name="name" attrs="{'readonly':[('state','!=','draft'),('state','!=','confirm')]}" invisible="1"/>
                        <field name="type" invisible="1"/>
                       </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
                </form>
            </field>
        </record>
        
        <record id="edit_feiertag_form" model="ir.ui.view">
            <field name="name">Leave Request</field>
            <field name="model">hr.holidays</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <form string="Leave Request" version="7.0">
                <header>
                    <button string="Confirm" name="confirm" states="draft" type="workflow" class="oe_highlight"/>
                    <button string="Approve" name="validate" states="confirm" type="workflow" groups="base.group_hr_manager" class="oe_highlight"/>
                    <button string="Validate" name="second_validate" states="validate1" type="workflow" groups="base.group_hr_manager" class="oe_highlight"/>
                    <button string="Refuse" name="refuse" states="confirm,validate1,validate" type="workflow" groups="base.group_hr_manager"/>
                    <button string="Reset to New" name="reset" states="refuse"/>
                    <field name="state" widget="statusbar" statusbar_visible="draft,confirm,validate" statusbar_colors='{"confirm":"blue","validate1":"blue","refuse":"red"}'/>
                </header>
                <sheet string="Leave Request">
                    <group style="width:30%%">
                        <field name="employee_id" required="1" on_change="onchange_employee(employee_id)"/>
                        <field name="date_from" string="Datum" widget="date" required="1" />
                        <field name="holiday_status_id" invisible="1" context="{'employee_id':employee_id}"/>
                        <field name="name" placeholder="z.B. Pfingstmontag" attrs="{'readonly':[('state','!=','draft'),('state','!=','confirm')]}"/>
                        <field name="type" invisible="1"/>
                       </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="holidays_tree">
            <field name="name">hr.holidays.tree</field>
            <field name="model">hr.holidays</field>
            <field name="arch" type="xml">
                <tree fonts="bold:message_unread==True" colors="red:state == 'refuse';blue:state == ' draft';black:state in ('confirm','validate','validate1')" string="Leave Requests">
                    <field name="employee_id"/>
                    <field name="name"/>
                    <field name="date_from" widget="date"/>
                    <field name="date_to" widget="date"/>
                    <field name="state"/>
                    <field name="leave_code" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                    <field name="message_unread" invisible="1"/>
                </tree>
            </field>
        </record>
        
        <record id="holidays_filter" model="ir.ui.view">
            <field name="name">hr.holidays.filter</field>
            <field name="model">hr.holidays</field>
            <field name="arch" type="xml">
                <search string="Search Leave">
                    <field name="name"/>
                    <separator/>                    
                    <filter icon="terp-check" domain="[('state','=','draft')]" string="To Confirm"/>
                    <filter icon="terp-camera_test" domain="[('state','in',('confirm','validate1'))]" string="To Approve" name="approve"/>
                    <filter icon="terp-camera_test" domain="[('state','=','validate')]" string="Validated" name="validated"/>
                    <separator/>
                    <filter icon="terp-go-year" name="year" string="Year" domain="[('holiday_status_id.active','=',True)]" help="Filters only on allocations and requests that belong to an holiday type that is 'active' (active field is True)"/>
                    <filter string="Unread Messages" name="message_unread" domain="[('message_unread','=',True)]" help="Unread messages"/>
                    <separator/>
                    <filter string="My Leaves" icon="terp-personal" name="my_leaves" domain="[('employee_id.user_id','=', uid)]" help="My Leaves"/>
                    <filter string="My Leaves &amp; To Approve" icon="terp-personal" name="my_leaves_a_to_approve" domain="['|',('state','=','confirm'),('employee_id.user_id','=', uid)]" help="My Leaves &amp;l To Approve"/>
                    <separator/>
                    <field name="employee_id"/>
                    <field name="holiday_status_id"/>
                    <group expand="0" string="Group By...">
                        <filter name="group_name" string="Description" domain="[]" context="{'group_by':'name'}"/>
                        <filter name="group_date_from" string="Start Date" icon="terp-personal" domain="[]" context="{'group_by':'date_from'}"/>
                        <filter name="group_employee" string="Employee" icon="terp-personal" domain="[]" context="{'group_by':'employee_id'}"/>
                        <filter string="Manager" icon="terp-personal" domain="[]" context="{'group_by':'manager_id'}"/>
                        <filter string="Status" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'state'}"/>
                    </group>
                </search>                
            </field>
            <field name="priority">0</field>            
        </record>
        
        

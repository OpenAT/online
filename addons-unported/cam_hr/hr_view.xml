            <field name="arch" type="xml">
               <form string="Public Holiday Template" version="7.0">
                   <sheet>
                       <div class="oe_title">
                           <label for="name" class="oe_edit_only"/>
                           <h1>
                               <field name="name"/>
                           </h1>
                       </div>
                       <div class="oe_right oe_button_box" name="button_box">
                          <button name="%(action_wizard_import_feiertage)d" string="Create Public Holidays" type="action"/>
                       </div>
                       <group>
                           <field name="sequence"/>
                           <field name="create_employees"/>
                       </group>
                       <group string="Public Holidays">
                       		<field name="line_ids" nolabel="1">
                       			<tree editable="bottom">
                       				<field name="name"/>
                       				<field name="date"/>
                       			</tree>
                       		</field>
                       </group>
                   </sheet>
               </form>
            </field>
        </record>
        

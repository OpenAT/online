<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- DONATION PRODUCT TEMPLATE FORM -->
        <record model="ir.ui.view" id="fso_product_template_form">
            <field name="name">FS-Online Product Template Form</field>
            <field name="model">product.template</field>
            <field name="arch" type="xml">
                <form string="Donations, Products, Sponsorships, Events, Memberships">
                    <!-- Technical fields -->
                    <field name="is_product_variant" invisible="1"/>
                    <field name='product_variant_count' invisible='1'/>
                    <!-- PRODUCT SHEET -->
                    <sheet>

                        <!-- HEAD (Button Box Name and Image) -->
                        <div class="oe_right oe_button_box" style="width: 400px;" name="buttons">
                            <p style="display: none;">Button Box Placeholder</p>
                        </div>

                        <div class="oe_left" style="width: calc(100% - 430px);">
                            <field name="image_medium" widget="image" class="oe_avatar oe_left"/>
                            <div style="margin-left: 140px; margin-top: 12px;">
                                <div class="oe_edit_only">
                                    <label for="name" string="Name"/>
                                </div>
                                <h1>
                                    <field name="name" required="True"/>
                                </h1>
                                <div class="oe_edit_only">
                                    <label for="description_sale" string="Short Description"/>
                                </div>
                                <h3>
                                    <field name="description_sale"/>
                                </h3>
                                <div class="oe_edit_only">
                                    <label for="website_url" string="Default URL"/>
                                </div>
                                <h4>
                                    <field name="website_url" invisible="0" widget="url" readonly="1"
                                           string="Default URL"/>
                                </h4>
                            </div>
                        </div>

                        <group name="Basic Settings">
                            <group>
                                <field name="fs_product_type" string="Fundraising Studio Type"/>
                                <field name="type"/>
                            </group>
                            <group>
                                <field name="uom_id" string="Unit of Measure"/>
                            </group>
                        </group>

                        <!-- TABS -->
                        <notebook>
                            <page string="Control">
                                <group>
                                    <group>
                                        <field name="website_published" string="Published"/>
                                        <field name="website_published_start" string="Publish Start"/>
                                        <field name="website_published_end" string="Publish End"/>
                                        <!-- !!! WARNING: fs_group_ids is deprecated !!! -->
                                        <!--<field name="fs_group_ids" string="Fundraising Studio Gruppen" widget="many2many_tags"/>-->
                                    </group>
                                    <group>
                                        <field name="website_visible" string="Visible (computed)" readonly="1"/>
                                    </group>
                                </group>
                                <group string="Fundraising Studio Groups">
                                    <field name="zgruppedetail_ids" string="Fundraising Studio Groups"
                                           widget="many2many_tags"
                                           options="{'no_open': True, 'no_create': True, 'no_create_edit': True, 'no_quick_create': True,}"/>
                                </group>
                                <group string="Search Engine Optimization">
                                    <field name="seo_url" string="Forced SEO URL"/>
                                </group>
                                <group string="Thank You Page">
                                    <field name="redirect_url_after_form_feedback" string="Redirect URL after PP Form-Feedback"/>
                                </group>
                                <group string="Donation Cart Settings (Shopping Cart)">
                                    <field name="clear_cart" string="Remove other Products"/>
                                    <field name="simple_checkout" string="Skip Donation Cart"/>
                                </group>
                            </page>

                            <page string="Payment">
                                <group>
                                    <group string="Amount" col="2">
                                        <field name="list_price" string="Default Amount"/>
                                        <field name="price_donate" string="Arbitrary Amount"/>
                                        <field name="price_donate_min" string="Minimal Amount"
                                               attrs="{'invisible': [('price_donate','=', False)]}"/>
                                    </group>
                                    <group col="2">
                                        <field name="donation_input_template" string="Donation Buttons Template"/>
                                        <field name="price_suggested_ids" string="Suggested Values (Donation Buttons)"
                                               attrs="{'invisible': [('price_donate','=', False)]}" colspan="2">
                                            <tree string="Suggested Donation-Values" default_order="sequence"
                                                  editable="bottom">
                                                <field name="sequence" widget="handle"/>
                                                <field name="name"/>
                                                <field name="amount"/>
                                                <field name="css_classes"/>
                                                <field name="clearfix"/>
                                                <field name="arbitrary_price"/>
                                            </tree>
                                        </field>
                                    </group>
                                </group>

                                <group string="Payment Interval" attrs="{'invisible': [('price_donate','=', False)]}">
                                    <div colspan="2" class="backend-help oe_edit_only">
                                        <p>Leave this empty for non-recurring donations or products.
                                        </p>
                                    </div>
                                    <group>
                                        <field name="payment_interval_default" string="Default Payment Interval"/>
                                        <field name="auto_recompute_price_donate" string="Auto recompute Price Donate"/>
                                    </group>
                                    <group>
                                        <label colspan="2" for="payment_interval_lines_ids"
                                               string="Payment Interval Choices"/>
                                        <field colspan="2" name="payment_interval_lines_ids" nolabel="1">
                                            <tree string="Payment Intervals" default_order="sequence" editable="bottom">
                                                <field name="sequence" widget="handle"/>
                                                <field name="payment_interval_id"/>
                                            </tree>
                                        </field>
                                    </group>
                                </group>

                                <!-- group string="Zahlungsmethoden" attrs="{'invisible': [('product_page_template','!=', 'website_sale_donate.ppt_opc')]}" -->
                                <group string="Payment Methods">
                                    <div colspan="2" class="backend-help oe_edit_only">
                                        <p>
                                            Override global payment configuration for this donation or product.
                                            Note that this only works if "Design Template" (Template Settings) is set to
                                            "Donation One-Page-Checkout".
                                        </p>
                                    </div>
                                    <!-- label colspan="2" for="product_acquirer_lines_ids" class="oe_edit_only"/ -->
                                    <field colspan="2" name="product_acquirer_lines_ids" nolabel="1"
                                           attrs="{'readonly': [('product_page_template','!=', 'website_sale_donate.ppt_opc')]}">
                                        <tree string="Acquirers" default_order="sequence" editable="bottom">
                                            <field name="sequence" widget="handle"/>
                                            <field name="acquirer_id"/>
                                            <field name="acquirer_pre_msg"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>

                            <page string="Template Settings">
                                <group>
                                    <group>
                                        <field name="product_page_template" string="Structural Template"/>
                                        <field name="website_theme" string="Design Template"/>
                                    </group>
                                    <group>
                                        <div class="text-center oe_edit_only">
                                            <div style="display: inline-block; max-width: 300px;">
                                                <img src="/fsonline/static/src/img/ppt_donate.jpg"
                                                     class="img-responsive"
                                                     attrs="{'invisible':[('product_page_template', '!=', 'website_sale_donate.ppt_donate')]}"/>

                                                <img src="/fsonline/static/src/img/ppt_opc.jpg"
                                                     class="img-responsive"
                                                     attrs="{'invisible':[('product_page_template', '!=', 'website_sale_donate.ppt_opc')]}"/>

                                                <img src="/fsonline/static/src/img/ppt_ahch.jpg"
                                                     class="img-responsive"
                                                     attrs="{'invisible':[('product_page_template', '!=', 'website_sale_donate.ppt_ahch')]}"/>

                                                <img src="/fsonline/static/src/img/ppt_inline_donation.jpg"
                                                     class="img-responsive"
                                                     attrs="{'invisible':[('product_page_template', '!=', 'website_sale_donate.ppt_inline_donation')]}"/>
                                            </div>
                                        </div>
                                    </group>
                                </group>
                                <notebook>
                                    <page string="Page Settings">
                                        <group>
                                            <group string="Page Settings" class="fso_backend_separator">
                                                <!-- h3 colspan="2" class="fso_backend_separator">Seiteneinstellungen</h3 -->
                                                <field name="hide_categories" string="Hide Category Navigation"/>
                                                <field name="hide_search" string="Hide Search"/>
                                                <field name="show_desctop" string="Extra Widget Area at Page Start"/>
                                                <field name="show_descbottom" string="Extra Widget Area at Page End"/>
                                                <field name="hide_salesdesc" string="Hide Short Description"/>
                                                <field name="format"
                                                       attrs="{'invisible': [('product_page_template','!=', 'website_sale_donate.ppt_ahch')]}"/>
                                                <field name="alternative_product_ids"
                                                       string="Linked Donation or Product pages" widget="many2many_tags"/>
                                                <field name="parallax_image_medium" string="Page Background" widget="image"
                                                       class="oe_avatar oe_left"/>
                                                <field name="parallax_speed" string="Background Scroll Speed (Parallax)"
                                                       attrs="{'invisible': [('parallax_image_medium','=', False)]}"/>
                                            </group>
                                            <group string="Donation Box Settings" class="fso_backend_separator">
                                                <!-- h3 colspan="2" class="fso_backend_separator">Spendenbox auf der Seite</h3 -->
                                                <field name="hide_payment" string="Hide Donation Box"/>
                                                <field name="hide_image" string="Hide Image"/>
                                                <!-- field name="image_square" string="Bild (autom. quadratisch zugeschnitten)" widget="image" class="oe_avatar oe_left" attrs="{'readonly': True, 'invisible': [('hide_image','=', True)]}"/ -->
                                                <field name="variants_as_list"
                                                       string="Show Product Variants as Selection List"/>
                                                <field name="payment_interval_as_selection"
                                                       string="Show Payment Intervals as Selection List"/>
                                                <field name="hide_quantity" string="Hide Quantity Selection"/>
                                                <field name="hide_amount_title" string="Hide Amount Title"/>
                                                <field name="amount_title" string="Differing Amount Title"
                                                       attrs="{'invisible': [('hide_amount_title','=', True)]}"/>
                                                <field name="hide_panelfooter" string="Hide Global Footer"/>
                                            </group>
                                            <group string="Donation Button" colspan="2">
                                                <field name="button_addtocart_text" string="Button Text"/>
                                            </group>
                                            <div colspan="4" class="backend-help oe_edit_only">
                                                <p>
                                                    Template settings define the look and feel of the website
                                                    as well as the the available features!
                                                </p>
                                            </div>
                                        </group>
                                    </page>

                                    <page string="Form Fields">
                                        <group string="Custom Checkout Fields">
                                            <field name="checkout_form_id" string="Checkout Fields Form"/>
                                            <button name="create_checkout_fields_form" type="object"
                                                    string="Setup custom Checkout Fields"
                                                    colspan="2"
                                                    attrs="{'invisible': [('checkout_form_id', '!=', False)]}"
                                            />
                                        </group>
                                    </page>

                                    <page string="Shop Steps"
                                          attrs="{'invisible':[('product_page_template', '=', 'website_sale_donate.ppt_opc')]}">

                                        <group name="steps_and_donation_buttons" string="Shop Steps">
                                            <group colspan="2" name="step_indicator_setup">
                                                <field name="step_indicator_setup" string="Individual Step-Indicator Setup"/>
                                            </group>
                                            <group colspan="2" name="step_indicator_setup_settings"
                                                   string="List Classes (UL Elements)"
                                                   class="fso_backend_separator"
                                                   attrs="{'readonly': [('step_indicator_setup','=', False)]}">
                                                <field name="step_indicator_ul_class"
                                                       string="CSS Classes"
                                                       attrs="{'readonly': [('step_indicator_setup','=', False)]}"/>
                                            </group>
                                            <group colspan="2" string="Cart Indicator" class="fso_backend_separator">
                                                <field name="hide_cart_indicator" string="Hide Step"
                                                       attrs="{'readonly': [('step_indicator_setup','=', False)]}"/>
                                                <field name="cart_indicator_name" string="Step Name"
                                                       attrs="{'readonly': ['|',
                                                                 ('hide_cart_indicator','=', True),
                                                                 ('step_indicator_setup','=', False)]}"/>
                                            </group>

                                            <group colspan="2" string="Product Indicator" class="fso_backend_separator">
                                                <field name="hide_product_indicator" string="Hide Step"
                                                       attrs="{'readonly': [('step_indicator_setup','=', False)]}"/>
                                                <field name="product_indicator_name" string="Step Name"
                                                       attrs="{'readonly': ['|',
                                                                 ('hide_product_indicator','=', True),
                                                                 ('step_indicator_setup','=', False)]}"/>
                                            </group>

                                            <group colspan="2" string="Checkout Indicator" class="fso_backend_separator">
                                                <field name="hide_checkout_indicator" string="Hide Step"
                                                       attrs="{'readonly': [('step_indicator_setup','=', False)]}"/>
                                                <field name="checkout_indicator_name" string="Step Name"
                                                       attrs="{'readonly': ['|',
                                                                 ('hide_checkout_indicator','=', True),
                                                                 ('step_indicator_setup','=', False)]}"/>
                                            </group>

                                            <group colspan="2" string="Payment Indicator" class="fso_backend_separator">
                                                <field name="hide_payment_indicator" string="Hide Step"
                                                       attrs="{'readonly': [('step_indicator_setup','=', False)]}"/>
                                                <field name="payment_indicator_name" string="Step Name"
                                                       attrs="{'readonly': ['|',
                                                                 ('hide_payment_indicator','=', True),
                                                                 ('step_indicator_setup','=', False)]}"/>
                                            </group>

                                            <group colspan="2" string="Confirmation Indicator" class="fso_backend_separator">
                                                <field name="hide_confirmation_indicator" string="Hide Step"
                                                       attrs="{'readonly': [('step_indicator_setup','=', False)]}"/>
                                                <field name="confirmation_indicator_name" string="Step Name"
                                                       attrs="{'readonly': ['|',
                                                                 ('hide_confirmation_indicator','=', True),
                                                                 ('step_indicator_setup','=', False)]}"/>
                                            </group>
                                        </group>
                                    </page>

                                    <page string="Donation Target"
                                          attrs="{'invisible': [('fs_product_type','not in', ['donation', 'mediation'])]}">
                                        <group string="Donation Target"
                                               attrs="{'invisible': [('fs_product_type','not in', ['donation', 'mediation'])]}">
                                            <group>
                                                <field name="funding_goal"/>
                                                <field name="funding_user"/>
                                            </group>
                                            <group>
                                                <field name="funding_reached" readonly="1"/>
                                            </group>
                                        </group>
                                        <group string="Page Settings">
                                            <group string="Left Area" class="fso_backend_separator">
                                                <field name="hide_fundingtext" string="Hide Short Description"/>
                                                <field name="hide_fundingbar" string="Hide Progress Bar"/>
                                                <field name="hide_fundingdesc" string="Hide Description"/>
                                                <field name="funding_desc" string="Description"/>
                                            </group>
                                            <group string="Donation Box" class="fso_backend_separator">
                                                <field name="hide_fundingtextincp" string="Hide Text"/>
                                                <field name="hide_fundingbarincp" string="Hide Progress Bar"/>
                                            </group>
                                        </group>
                                        <group string="Product Overview Pages">
                                            <group>
                                                <field name="hide_fundingtextinlist" string="Hide Text"/>
                                                <field name="hide_fundingbarinlist" string="Hide Progress Bar"/>
                                            </group>
                                            <group>

                                            </group>
                                        </group>
                                    </page>

                                    <page string="Gift Donations">
                                        <group>
                                            <group string="Webshop: Enable Gifting Feature">
                                                <field name="giftee_form_id" string="Giftee Fields Form"/>
                                                <button name="create_giftee_fields_form" type="object"
                                                        string="Enable Giftee Fields"
                                                        colspan="2"
                                                        attrs="{'invisible': [('giftee_form_id', '!=', False)]}"
                                                />
                                                <field name="giftee_checkbox_text"
                                                       attrs="{'invisible': [('giftee_form_id', '=', False)]}"/>
                                                <field name="giftee_email_template"
                                                       attrs="{'invisible': [('giftee_form_id', '=', False)]}"/>
                                            </group>
                                            <group>
                                                <div class="text-center oe_edit_only">
                                                    <div style="display: inline-block; max-width: 300px;">
                                                        <img src="/fsonline/static/src/img/gift_donations.jpg"
                                                             class="img-responsive"/>
                                                    </div>
                                                </div>
                                            </group>
                                        </group>
                                    </page>
                                </notebook>
                            </page>

                            <page string="Shop Categories">
                                <group string="Product List Settings">
                                    <field name="website_sequence" string="Sequence"/>
                                    <field name="public_categ_ids" string="Show in Categories"
                                           widget="many2many_tags"/>
                                    <field name="website_style_ids" string="Design (Additional CSS Classes)"
                                           widget="many2many_tags"/>
                                    <field name="do_not_link" string="Do not Link to Donation Page"/>
                                    <field name="hide_price" string="Hide Amount"/>
                                </group>
                            </page>

                            <page string="Digital Contents">
                                <group string="Digital Contents"
                                       attrs="{'invisible': [('product_page_template','==', 'website_sale.product')]}">
                                    <field name="webshop_download_file" string="Download File" widget="binary"
                                           filename="webshop_download_file_name"/>
                                    <field name="webshop_download_file_name" string="Download Filename"/>
                                </group>
                            </page>

                            <page name="variants" string="Variants">
                                <div class="oe_right">
                                    <button class="oe_inline oe_stat_button" string="Variant Prices"
                                            name="%(product.variants_template_action)d" type="action"
                                            icon="fa-strikethrough"/>
                                    <button class="oe_inline oe_stat_button" name="%(product.product_variant_action)d"
                                            type="action" icon="fa-sitemap">
                                        <field string="List of Variants" name="product_variant_count"
                                               widget="statinfo"/>
                                    </button>
                                </div>
                                <p class="oe_grey">
                                    <strong>Warning</strong>: adding or deleting attributes
                                    will delete and recreate existing variants and lead
                                    to the loss of their possible customizations.
                                </p>
                                <field name="attribute_line_ids" widget="one2many_list"
                                       context="{'show_attribute': False}">
                                    <tree string="Variants" editable="bottom">
                                        <field name="attribute_id"/>
                                        <field name="value_ids" widget="many2many_tags"
                                               options="{'no_create_edit': True}"
                                               domain="[('attribute_id', '=', attribute_id)]"
                                               context="{'default_attribute_id': attribute_id}"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="Accessory Products">
                                <group>
                                    <field name="accessory_product_ids" string="Accessory Donations and Products"
                                           widget="many2many_tags"/>
                                </group>
                            </page>

                            <page string="System">
                                <group string="Information">
                                    <group>
                                        <field name="active"/>
                                        <field name="state"/>
                                        <field name="product_manager"/>
                                        <field name="ean13"/>
                                        <field name="default_code" string="Internal Code"/>
                                        <field name="description" string="Internal Description"/>
                                    </group>
                                    <group>
                                        <field name="id" readonly="True"/>
                                        <field name="create_date" readonly="True"/>
                                        <field name="create_uid" readonly="True"/>
                                        <field name="write_date" readonly="True"/>
                                        <field name="write_uid" readonly="True"/>
                                        <!-- WARNING: fs_workflow is deprecated use fs_product_type instead !-->
                                        <!--<field name="fs_workflow" string="FS Workflow (veraltet)"/>-->
                                    </group>
                                </group>

                                <group string="Accounting">
                                    <group>
                                        <field name="categ_id"/>
                                        <div colspan="2" class="backend-help oe_edit_only">
                                            <p>
                                                This category specifies the target account in accounting, unless
                                                this product has an explicit accounts set. This category is unrelated
                                                to website categories.
                                            </p>
                                        </div>
                                    </group>
                                    <group>
                                    </group>
                                </group>
                                <group>
                                    <group>
                                        <field name="property_account_income" domain="[('type', '!=', 'view')]"/>
                                        <field name="taxes_id" widget="many2many_tags"/>
                                    </group>
                                    <group>
                                        <field name="property_account_expense" domain="[('type', '!=', 'view')]"/>
                                        <field name="supplier_taxes_id" widget="many2many_tags"/>
                                    </group>
                                </group>

                                <group string="Procurement">
                                    <group name="Supply Chain Information">
                                        <field name="standard_price"/>
                                        <field name="description_purchase"/>
                                    </group>
                                    <group name="Supply Chain">
                                        <field name="route_ids" widget="many2many_checkboxes"/>
                                        <field name="seller_ids"/>
                                    </group>
                                </group>

                                <group string="Stock" attrs="{'invisible':[('type','=','service')]}">
                                    <group name="Inventory Assessment" class="fso_backend_separator"
                                           groups="base.group_user">
                                        <label for="qty_available"/>
                                        <div>
                                            <field name="qty_available" class="oe_inline"/>
                                            <button name="%(stock.action_view_change_product_quantity)d"
                                                    string="⇒ Update"
                                                    type="action"
                                                    class="oe_link"/>
                                        </div>
                                        <label for="incoming_qty"/>
                                        <div>
                                            <field name="incoming_qty" class="oe_inline"/>
                                            <button string="⇒ Request Procurement" name="%(stock.act_make_procurement)d"
                                                    type="action" class="oe_link"/>
                                        </div>
                                        <field name="virtual_available"/>
                                    </group>
                                    <group name="Counter-Part Locations Properties" groups="stock.group_locations"
                                           class="fso_backend_separator">
                                        <field name="property_stock_procurement"
                                               domain="[('usage','=','procurement')]"/>
                                        <field name="property_stock_production" domain="[('usage','=','production')]"/>
                                        <field name="property_stock_inventory" domain="[('usage','=','inventory')]"/>
                                    </group>
                                </group>
                                <group name="Lager_2" attrs="{'invisible':[('type','=','service')]}">
                                    <group name="Volume and Weight" class="fso_backend_separator">
                                        <field name="volume"/>
                                        <field name="weight"/>
                                        <field name="weight_net"/>
                                    </group>
                                    <group name="Storage Location" class="fso_backend_separator">
                                        <field name="loc_rack"/>
                                        <field name="loc_row"/>
                                        <field name="loc_case"/>
                                    </group>
                                </group>

                            </page>
                        </notebook>
                    </sheet>

                </form>
            </field>
        </record>

        <!-- DONATION PRODUCT TEMPLATE TREE -->
        <record model="ir.ui.view" id="fso_product_template_tree">
            <field name="name">FS-Online Product Template Tree</field>
            <field name="model">product.template</field>
            <field name="arch" type="xml">
                <tree string="Donations and Products"
                      default_order="website_sequence"
                      colors="red:not fs_product_type and website_published;
                          grey:not fs_product_type;
                          green:website_published and website_visible;
                          orange:website_published and not website_visible;
                          grey:not website_published;">
                    <field name="website_sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="fs_product_type" string="FS Type"/>
                    <field name="website_theme" string="Theme"/>
                    <field name="price_donate" string="Arbitrary Amount"/>
                    <field name="list_price" string="Amount"/>
                    <field name="payment_interval_default" string="Def. Interv."/>
                    <!-- WARNING: fs_group_ids is deprecated !"-->
                    <!--<field name="fs_group_ids" string="FS Gruppen" widget="many2many_tags"/>-->
                    <field name="zgruppedetail_ids" string="FS Groups" widget="many2many_tags"/>
                    <field name="product_page_template" string="Web template"/>
                    <field name="website_published" string="Published"/>
                    <field name="website_published_start" string="Pub. Start"/>
                    <field name="website_published_end" string="Pub. End"/>
                    <field name="website_visible" string="Visible"/>
                    <field name="has_widget_manager" invisible="1" />
                </tree>
            </field>
        </record>

        <record id="fso_product_template_tree_inherited" model="ir.ui.view">
            <field name="name">FS-Online Product Template Tree Inherited</field>
            <field name="inherit_id" ref="fsonline.fso_product_template_tree"/>
            <field name="model">product.template</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <field name="website_visible" position="after">
                    <button icon="STOCK_FILE"
                            name="view_product_page"
                            string="View Product Page"
                            type="object"/>
                    <button icon="stock_gantt"
                            name="view_widget_manager"
                            string="View Widget Manager"
                            type="object"
                            attrs="{'invisible': [('has_widget_manager', '=', False)]}"/>
                    <button icon="stock_graph"
                            name="view_product_statistics"
                            string="View Product Statistics"
                            type="object"/>
                </field>
            </field>
        </record>

        <record model="ir.actions.act_window" id="fso_product_template_viewaction">
            <field name="name">Donations and Products</field>
            <field name="res_model">product.template</field>
            <field name="view_mode">tree, form</field>

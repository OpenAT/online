<?xml version="1.0" encoding="utf-8"?>
<openerp><data>

    <record id="survey_options_form" model="ir.ui.view">
        <field name="name">Form view for survey options</field>
        <field name="model">survey.survey</field>
        <field name="inherit_id" ref="survey.survey_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quizz_mode']" position="after">
                <field name="css_class"/>
                <field name="start_button_text"/>
            </xpath>
        </field>
    </record>

</data></openerp>

"            ${object.partner_id.street}<br/>\n"
"        % endif\n"
"        % if object.partner_id.street2:\n"
"            ${object.partner_id.street2}<br/>\n"
"        % endif\n"
"        % if object.partner_id.city or object.partner_id.zip:\n"
"            ${object.partner_id.zip} ${object.partner_id.city}<br/>\n"
"        % endif\n"

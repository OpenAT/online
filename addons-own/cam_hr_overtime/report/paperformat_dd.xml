<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="paperformat_dd" model="report.paperformat">
            <field name="name">European A4 Hofer Timesheet</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">7</field>
            <field name="margin_bottom">7</field>
            <!-- <field name="margin_bottom">5</field> -->
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">0</field>
            <field name="dpi">90</field>
        </record>

        <record id="cam_hr_overtime.report_overtime_qweb_neu" model="ir.actions.report.xml">
            <field name="paperformat_id" ref="cam_hr_overtime.paperformat_dd" />
        </record>

    </data>
</openerp>
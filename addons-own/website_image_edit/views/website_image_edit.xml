<?xml version="1.0" encoding="utf-8"?>
<!--wie = website_image_edit-->
<openerp>
    <data>
        <template id="website_image_edit.assets_editor" inherit_id="website.assets_editor" name="Frontend Image edit with darkroom.js" groups="base.group_website_publisher">
            <xpath expr='//script[@src="/website/static/src/js/website.translator.js"]' position="after">

                <!--Darkroom Library files-->
                <script src="/website_image_edit/static/lib/js/fabric.js"/>

                <script src="/website_image_edit/static/lib/darkroomjs/lib/core/bootstrap.js"/>
                <script src="/website_image_edit/static/lib/darkroomjs/lib/core/darkroom.js"/>
                <script src="/website_image_edit/static/lib/darkroomjs/lib/core/plugin.js"/>
                <script src="/website_image_edit/static/lib/darkroomjs/lib/core/transformation.js"/>
                <script src="/website_image_edit/static/lib/darkroomjs/lib/core/ui.js"/>
                <script src="/website_image_edit/static/lib/darkroomjs/lib/core/utils.js"/>

                <!--Image edit functions-->
                <script src="/website_image_edit/static/src/js/plugins/darkroom.history.js"/>
                <script src="/website_image_edit/static/src/js/plugins/darkroom.crop.js"/>
                <script src="/website_image_edit/static/src/js/plugins/darkroom.rotate.js"/>
                <script src="/website_image_edit/static/src/js/plugins/darkroom.mirror.js"/>
                <script src="/website_image_edit/static/src/js/plugins/darkroom.circle.js"/>
                <script src="/website_image_edit/static/src/js/plugins/darkroom.rounded.js"/>

                <!--Extension of Imagedialog-->
                <link id="wie_css" rel='stylesheet' href='/website_image_edit/static/src/css/website_image_edit.css' t-ignore="true"/>
                <script id="wie_script" type="text/javascript" src="/website_image_edit/static/src/js/website_image_edit.js"/>
                <script id="wie_load_templates" type="text/javascript" src="/website_image_edit/static/src/js/load_templates.js"></script>
            </xpath>
        </template>
    </data>
</openerp>
